	<customtag:info_message titleKey="Help.not_implemented.secondStep.title" 
		key="Help.not_implemented.secondStep.desc" status="INFO" icon_id="12" style="margin: 20px;"/>
		
	<customtag:info_message titleKey="Help.not_implemented.thirdStep.title" 
		key="Help.not_implemented.thirdStep.desc" status="INFO" icon_id="12" style="margin: 20px;"/>
	
	<customtag:info_message titleKey="Help.not_implemented.fourthStep.title" 
		key="Help.not_implemented.fourthStep.desc" status="INFO" icon_id="12" style="margin: 20px;"/>
	
	<customtag:info_message titleKey="Help.not_implemented.fifthStep.title" 
		key="Help.not_implemented.fifthStep.desc" status="INFO" icon_id="12" style="margin: 20px;"/>
	
	<customtag:info_message titleKey="Help.not_implemented.sixthStep.title" 
		key="Help.not_implemented.sixthStep.desc" status="INFO" icon_id="13" style="margin: 20px;">
		<p><html:link action="/ranking.do"><fmt:message key="Ranking.tagName"/></html:link></p>
	</customtag:info_message>
	</customtag:window>
	
</jsp:root>