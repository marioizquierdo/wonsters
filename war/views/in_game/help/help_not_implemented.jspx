<jsp:root 	xmlns="http://www.w3.org/1999/xhtml"
          	xmlns:jsp="http://java.sun.com/JSP/Page"
          	xmlns:fmt="http://java.sun.com/jsp/jstl/fmt"
			xmlns:customtag="urn:jsptagdir:/WEB-INF/tags"
          	xmlns:html="http://struts.apache.org/tags-html"
          	version="2.0">

	
<customtag:window id="HelpNotImplemented" title="Help.title">
	<div id="not_implemented_page_image_wrapper">
	   <img src="images/buildings/classroom_BIG.jpg" width="327" height="170" class="ui-corner-all" />
	</div>
	
	<p class="info_slot">
		<fmt:message key="NotImplementedHelp.apologize"/>
